page,
.page-view {
  height: 100%;
  position: relative;
  background: #fff;
}
.group {
  margin: 0 40rpx;
  padding-top: 20rpx;
}
.group .row {
  font-size: 32rpx;
}
.group .row .name {
  line-height: 66rpx;
  margin-right: 20rpx;
  font-weight: 500;
}
.group .row + .row {
  margin-top: 20rpx;
}
.group input, .group picker {
  color: #999999;
  height: 66rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  border: 2rpx solid #aaaaaa;
  padding-left: 20rpx;
}
.group .select {
  position: relative;
}
.group .select input, .group .select .uni-input {
  color: #333333;
}
.group .select .uni-input {
  height: 66rpx;
  line-height: 66rpx;
}
.group .select .down {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  right: 20rpx;
}
.group .margin-top-lg {
  margin-top: 60rpx;
}
.estimate-result {
  margin: 20rpx 40rpx 0;
}
.estimate-result .title {
  font-size: 32rpx;
  font-weight: 500;
}
.estimate-result .result-con {
  margin: 20rpx 0 60rpx;
}
.estimate-result .result-con .tip {
  text-align: left;
}
.estimate-result .result-con .icon {
  width: 100rpx;
  display: block;
  margin: 0 auto;
  margin-bottom: 10rpx;
}
.estimate-result .result-con .warn {
  margin-top: 20rpx;
  background: #F2F2F2;
  padding: 10rpx 20rpx;
  line-height: 40rpx;
}
.tablebox {
  padding: 30rpx 0;
  color: #434040;
  font-size: 26rpx;
}
.tablebox .table {
  border: 1px solid #dbdbdb;
  text-align: center;
}
.tablebox .table .table-hd,
.tablebox .table .table-bd .table-td,
.tablebox .table .table-ft {
  -webkit-box-align: stretch;
  -webkit-align-items: stretch;
          align-items: stretch;
}
.tablebox .table .table-hd ._span,
.tablebox .table .table-bd .table-td ._span,
.tablebox .table .table-ft ._span {
  width: 93rpx;
  padding: 25rpx 0;
}
.tablebox .table .table-hd ._span:first-child,
.tablebox .table .table-bd .table-td ._span:first-child,
.tablebox .table .table-ft ._span:first-child {
  width: 130rpx;
}
.tablebox .table .table-hd ._span + ._span,
.tablebox .table .table-bd .table-td ._span + ._span,
.tablebox .table .table-ft ._span + ._span {
  border-left: 1px solid #dbdbdb;
}
.tablebox .table .table-hd {
  border-bottom: 1px solid #dbdbdb;
  height: 75rpx;
  background: #db6553;
  color: #ffffff;
  font-size: 28rpx;
  font-weight: bold;
}
.tablebox .table .table-bd .table-td {
  color: #6c6c6c;
  font-size: 25rpx;
}
.tablebox .table .table-bd .table-td + .table-td {
  border-top: 1px solid #dbdbdb;
}
.tablebox .table .table-bd .table-td:nth-child(even) {
  background: #f9f9f9;
}
.tablebox .table .table-bd {
  border-bottom: 1px solid #dbdbdb;
}
.tablebox .table .table-ft {
  color: #fd3c3e;
  font-size: 26rpx;
  height: 74rpx;
}
.tablebox .table .table-ft .text-right {
  color: #2e2e2e;
  font-size: 28rpx;
  font-weight: bold;
  padding-right: 54rpx;
  color: #2e2e2e;
}
.scan {
  margin: 40rpx 0;
  text-align: left;
  position: relative;
}
.scan .code {
  width: 260rpx;
  height: 260rpx;
  display: block;
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.scan .des {
  margin-left: 280rpx;
  font-size: 28rpx;
  line-height: 40rpx;
}
.scan .des .tip {
  margin-bottom: 10rpx;
}

